<?xml version="1.0" encoding="UTF-8"?>
<beans default-lazy-init="true" xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.2.xsd
            http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd">

    <context:property-override
            location="config-security-cda-overrides-test.properties"
            ignore-resource-not-found="true" />

    <bean id="cdaAuthenticationProvider" class="org.fao.geonet.kernel.security.cda.CdaAuthenticationProvider"/>
    <bean id="cdaConfiguration" class="org.fao.geonet.kernel.security.cda.CdaConfiguration">
        <property name="uri" value="${cdaConfiguration.uri}" />
    </bean>

    <bean id="servletContext" class="org.fao.geonet.GeonetMockServletContext"/>
    <bean id="threadPool" class="org.fao.geonet.TestThreadPool"/>
</beans>
